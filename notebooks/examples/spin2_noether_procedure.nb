Cell["\<\
MakeAction produces an action from a list of possible uncontracted terms. \
Total derivatives are removed.\
\>", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"MakeAction", "[", 
   RowBox[{"terms_List", ",", "label_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"contractions", ",", "tds", ",", "tdrules"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"contractions", " ", "=", " ", 
      RowBox[{"Union", "@", 
       RowBox[{"Flatten", "[", 
        RowBox[{"AllContractions", "/@", "terms"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"tds", " ", "=", " ", 
      RowBox[{"TotalDerivatives", "[", 
       RowBox[{"contractions", ",", 
        RowBox[{"H", "[", 
         RowBox[{
          RowBox[{"-", "a"}], ",", 
          RowBox[{"-", "b"}]}], "]"}], ",", "CD"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"tdrules", " ", "=", " ", 
      RowBox[{"First", "/@", 
       RowBox[{"TotalDerivativeRules", "[", "tds", "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"MakeAnsatz", "[", 
      RowBox[{
       RowBox[{"Complement", "[", 
        RowBox[{"contractions", ",", "tdrules"}], "]"}], ",", "label"}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"DefNiceConstantSymbol", "[", 
    RowBox[{"name_String", ",", "number_Integer"}], "]"}], ":=", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"symbol", "=", 
       RowBox[{"GiveSymbol", "[", 
        RowBox[{"name", ",", "number"}], "]"}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Quiet", "@", 
       RowBox[{"DefConstantSymbol", "[", 
        RowBox[{"symbol", ",", 
         RowBox[{"PrintAs", "\[Rule]", 
          RowBox[{"StringJoin", "[", 
           TagBox[
            StyleBox[
             RowBox[{"{", 
              RowBox[{"\"\<\\!\\(\\*SubscriptBox[\\(\>\"", ",", 
               RowBox[{"ToString", "@", "name"}], ",", "\"\<\\), \\(\>\"", 
               ",", 
               RowBox[{"ToString", "@", "number"}], ",", "\"\<\\)]\\)\>\""}], 
              "}"}],
             ShowSpecialCharacters->False,
             ShowStringCharacters->True,
