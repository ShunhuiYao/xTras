   RowBox[{
    RowBox[{"$CommuteCovDsOnScalars", "=", "True"}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"Don", "'"}], "t", " ", "print", " ", "info", " ", "when", " ", 
      "defining", " ", "objects", " ", "such", " ", "as", " ", "manifold"}], 
     ",", " ", "tensors", ",", " ", 
     RowBox[{"etc", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"$DefInfoQ", " ", "=", "False"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Don", "'"}], "t", " ", "print", " ", "info", " ", "when", " ", 
