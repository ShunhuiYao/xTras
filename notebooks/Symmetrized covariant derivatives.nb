      SubscriptBox["\[Del]", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["b", "1"], "..."}], 
         SubscriptBox["b", "m"]}], ")"}]]}], "=", 
     RowBox[{
      FractionBox["1", "m"], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[Del]", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            SubscriptBox["a", "1"], "..."}], 
