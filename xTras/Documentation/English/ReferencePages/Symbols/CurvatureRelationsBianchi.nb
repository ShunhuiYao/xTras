Cell[TextData[{
 "The second Bianchi identity for the Riemann tensor is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Del]", 
      RowBox[{"[", "a"}]], 
     SubscriptBox["R", 
      RowBox[{
       RowBox[{
        StyleBox["bc",
         FontSlant->"Italic"], "]"}], 
       StyleBox["de",
        FontSlant->"Italic"]}]]}], "=", "0"}], TraditionalForm]], 
  "InlineMath"],
 "."
}], "Notes",
 CellChangeTimes->{{3.580459516889176*^9, 3.580459659253481*^9}, {
  3.580460283689967*^9, 3.5804602842314463`*^9}},
 CellID->1067943069],

Cell[TextData[{
 "The identities returned by ",
 Cell[BoxData["CurvatureRelationsBianchi"], "InlineFormula",
  FormatType->"StandardForm"],
 " are the first contraction  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Del]", 
      RowBox[{"[", "a"}]], 
     SuperscriptBox[
      SubscriptBox["R", 
       RowBox[{
        RowBox[{
         StyleBox["bc",
          FontSlant->"Italic"], "]"}], 
        StyleBox["d",
         FontSlant->"Italic"]}]], "a"]}], "=", " ", 
    FormBox[
     RowBox[{
      RowBox[{
       SubscriptBox["\[Del]", "c"], 
       SubscriptBox["R", 
        StyleBox["bd",
         FontSlant->"Italic"]]}], "+", 
      RowBox[{
       SubscriptBox["\[Del]", "b"], 
       SubscriptBox["R", 
        StyleBox["cd",
         FontSlant->"Italic"]]}]}],
     TraditionalForm]}], TraditionalForm]], "InlineMath"],
 " and the second contraction ",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    FormBox[
     RowBox[{
      RowBox[{
       SubscriptBox["\[Del]", "a"], 
       SuperscriptBox[
        SubscriptBox["R", "b"], "a"]}], "=", " ", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{
        SubscriptBox["\[Del]", "b"], "R"}]}]}],
     TraditionalForm]}], TraditionalForm]], "InlineMath"],
 " of the second Bianchi identity."
}], "Notes",
 CellChangeTimes->{{3.580459737830357*^9, 3.5804597664080067`*^9}, {
  3.580459836275628*^9, 3.580459926013565*^9}, {3.580459966725613*^9, 
