Cell[CellGroupData[{

Cell["AllContractions", "ObjectName",
 CellID->1224892054],

Cell[TextData[{
 Cell["   ", "ModInfo"],
 Cell[BoxData[
  RowBox[{"AllContractions", "[", 
   StyleBox["expr", "TI"], "]"}]], "InlineFormula"],
 "\[LineSeparator]returns a sorted list of all possible full contractions of ",
 StyleBox["expr", "TI"],
 " over its free indices.\n",
 Cell["   ", "ModInfo"],
 Cell[BoxData[
  RowBox[{"AllContractions", "[", 
   RowBox[{
    StyleBox["expr", "TI"], ",", " ", 
    StyleBox["frees", "TI"]}], "]"}]], "InlineFormula"],
 "\[LineSeparator]returns all possible contractions of ",
 StyleBox["expr", "TI"],
 " that have ",
 Cell[BoxData[
  StyleBox["frees", "TI"]], "InlineFormula"],
 " as free indices.\n",
 Cell["   ", "ModInfo"],
 Cell[BoxData[
  RowBox[{"AllContractions", "[", 
   RowBox[{
    StyleBox["expr", "TI"], ",", " ", 
    StyleBox["frees", "TI"], ",", " ", 
    StyleBox["sym", "TI"]}], "]"}]], "InlineFormula"],
 "\[LineSeparator]returns all possible contractions of ",
 StyleBox["expr", "TI"],
 " with the symmetry ",
 StyleBox["sym", "TI"],
 " imposed on the free indices ",
 StyleBox["frees", "TI"],
 "."
}], "Usage",
 CellChangeTimes->{{3.579753723358705*^9, 3.5797537237619047`*^9}, {
  3.579754217957479*^9, 3.5797542234399*^9}, {3.579766875517914*^9, 
  3.579766963255599*^9}, {3.5797671360727043`*^9, 3.579767138391906*^9}, {
  3.579767356934888*^9, 3.579767380186286*^9}, {3.5797674107777348`*^9, 
  3.579767429332328*^9}, {3.579778768713862*^9, 3.579778769139179*^9}}],

Cell[TextData[{
 "The first argument can also be a list, in which case ",
 Cell[BoxData["AllContractions"], "InlineFormula"],
 " returns the union of all contractions for each of the members of the \
list."
}], "Notes",
 CellChangeTimes->{{3.5797672480604467`*^9, 3.579767271199193*^9}, {
  3.5797673082095337`*^9, 3.5797673346794367`*^9}},
 CellID->913824970],

Cell[TextData[{
 Cell[BoxData["AllContractions"], "InlineFormula"],
 " returns ",
 StyleBox["{}", "InlineCode"],
 " if it is not possible to take contractions, or throws an exception if it \
encounters an error."
}], "Notes",
 CellChangeTimes->{{3.585282786665802*^9, 3.5852828425239677`*^9}, {
  3.585282885043537*^9, 3.585282891937437*^9}},
 CellID->1941700588],

Cell["\<\
The indices in the second argument have to be given as an IndexList or as a \
List.\
\>", "Notes",
 CellChangeTimes->{{3.579767025623665*^9, 3.579767046940851*^9}, {
   3.57976708969366*^9, 3.5797670959330463`*^9}, 3.5797681671080093`*^9, {
   3.58210693698037*^9, 3.582106939786765*^9}},
 CellID->235029229],

Cell[TextData[{
 "The input expression ",
 StyleBox["expr", "TI"],
 " cannot have dummy indices."
}], "Notes",
 CellChangeTimes->{{3.579754141740531*^9, 3.579754148296451*^9}, {
  3.58210692370205*^9, 3.582106924619672*^9}},
 CellID->1895409007],

Cell[TextData[{
 "The number of free indices of ",
 StyleBox["expr", "TI"],
 " plus those in ",
 StyleBox["frees", "TI"],
 " has to be even."
}], "Notes",
 CellChangeTimes->{{3.579768185640726*^9, 3.579768192797247*^9}, {
  3.5821068834820967`*^9, 3.5821069030853367`*^9}},
 CellID->652395325],

Cell["\<\
The free indices of the input have to belong to the same tangent bundle.\
\>", "Notes",
 CellChangeTimes->{{3.579754170886713*^9, 3.57975418470009*^9}},
 CellID->366761051],

Cell["The metric of that tangent bundle has to be symmetric.", "Notes",
 CellChangeTimes->{{3.579754195757156*^9, 3.579754210344883*^9}},
 CellID->874917267],

Cell[TextData[{
 "The first argument can also be given in ",
