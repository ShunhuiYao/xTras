Cell[TextData[{
 StyleBox["xTras",
  FontSlant->"Italic"],
 " automatically defines 'proper' variations with respect to the metric via \
",
 Cell[BoxData["VarD"], "InlineFormula",
  FormatType->"StandardForm"],
 " and ",
 Cell[BoxData[
  ButtonBox["VarL",
   BaseStyle->"Link",
   ButtonData->"paclet:xTras/ref/VarL"]], "InlineFormula"],
 " whenever you define a metric."
}], "Text",
 CellChangeTimes->{{3.5809617794426622`*^9, 3.580961854709251*^9}, {
  3.5809621339817667`*^9, 3.580962154388667*^9}, {3.580962935742174*^9, 
  3.580962935958701*^9}},
 CellID->1534169418],

Cell[BoxData[GridBox[{
   {
    RowBox[{
     RowBox[{"VarD", "[", 
      RowBox[{
       RowBox[{
        StyleBox["g", "TI"], "[", 
        RowBox[{
         RowBox[{"-", 
          StyleBox["a", "TI"]}], ",", 
         RowBox[{"-", 
          StyleBox["b", "TI"]}]}], "]"}], ",", 
       StyleBox["cd", "TI"]}], "]"}], "[", 
     StyleBox["L", "TI"], "]"}], Cell[TextData[{
     StyleBox["returns ",
      Deletable->True],
     Cell[BoxData[
      FormBox[
       FractionBox[
        RowBox[{"\[Delta]", " ", "L", " "}], 
        RowBox[{"\[Delta]", " ", 
         SubscriptBox["g", 
          StyleBox["ab",
           FontSlant->"Italic"]]}]], TraditionalForm]], "InlineMath"],
     " while integrating by parts with respect to the covariant derivative ",
     StyleBox["cd", "TI"],
     "."
    }], "TableText"]},
   {
    RowBox[{
     RowBox[{
      ButtonBox["VarL",
       BaseStyle->"Link",
       ButtonData->"paclet:xTras/ref/VarL"], "[", 
      RowBox[{
       RowBox[{
        StyleBox["g", "TI"], "[", 
        RowBox[{
         RowBox[{"-", 
          StyleBox["a", "TI"]}], ",", 
         RowBox[{"-", 
          StyleBox["b", "TI"]}]}], "]"}], ",", 
       StyleBox["cd", "TI"]}], "]"}], "[", 
     StyleBox["L", "TI"], "]"}], Cell[TextData[{
     StyleBox["returns ",
      Deletable->True],
     Cell[BoxData[
      FormBox[
       RowBox[{
        FractionBox["1", 
         SqrtBox[
          RowBox[{"-", "g"}]]], 
        FractionBox[
         RowBox[{"\[Delta]", " ", 
          RowBox[{"(", 
           RowBox[{
            SqrtBox[
             RowBox[{"-", "g"}]], " ", "L"}], ")"}], " "}], 
         RowBox[{"\[Delta]", " ", 
          SubscriptBox["g", 
           StyleBox["ab",
            FontSlant->"Italic"]]}]]}], TraditionalForm]], "InlineMath"],
     " while integrating by parts with respect to the covariant derivative ",
     StyleBox["cd", "TI"],
     "."
    }], "TableText"]}
  }]], "DefinitionBox",
 CellChangeTimes->{{3.5809138828683157`*^9, 3.5809138873675547`*^9}, {
