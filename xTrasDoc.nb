Cell[CellGroupData[{

Cell["ClearAutomaticRules", "Subsection"],

Cell["\<\
In xAct, we can use AutomaticRules to attach rules to symbols and tensors. \
For example, you can automatically define a trace of tensor to be a new \
tensor:\
\>", "Text"],

Cell[BoxData[{
 RowBox[{"DefTensor", "[", 
  RowBox[{
   RowBox[{"H", "[", 
    RowBox[{
     RowBox[{"-", "a"}], ",", 
     RowBox[{"-", "b"}]}], "]"}], ",", "M", ",", 
   RowBox[{"Symmetric", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"-", "a"}], ",", 
      RowBox[{"-", "b"}]}], "}"}], "]"}]}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"DefTensor", "[", 
  RowBox[{
   RowBox[{"Ht", "[", "]"}], ",", "M", ",", 
   RowBox[{"PrintAs", "\[Rule]", "\"\<H\>\""}]}], "]"}]}], "Input"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"MakeRule", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"metric", "[", 
       RowBox[{"a", ",", "b"}], "]"}], 
      RowBox[{"H", "[", 
       RowBox[{
        RowBox[{"-", "a"}], ",", 
        RowBox[{"-", "b"}]}], "]"}]}], ",", 
     RowBox[{"Ht", "[", "]"}]}], "}"}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"MakeRule", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"H", "[", 
      RowBox[{
       RowBox[{"-", "a"}], ",", "a"}], "]"}], ",", 
     RowBox[{"Ht", "[", "]"}]}], "}"}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"rules", " ", "=", " ", 
  RowBox[{"Join", "[", 
   RowBox[{"%", ",", "%%"}], "]"}]}]}], "Input"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{
    RowBox[{"HoldPattern", "[", 
     InterpretationBox[
      StyleBox[GridBox[{
         {"H", 
          StyleBox[GridBox[{
             {" ", 
              UnderscriptBox[
               UnderscriptBox["a", "_"], "_"]},
